{
  "title": "NOVAX â€” Overview",
  "panels": [
    {
      "title": "Equity",
      "type": "timeseries",
      "targets": [{"expr":"novax_equity_total"}],
      "gridPos":{"x":0,"y":0,"w":12,"h":6}
    },
    {
      "title": "PnL total / Trades total",
      "type": "stat",
      "targets": [{"expr":"novax_pnl_total"},{"expr":"novax_trades_total"}],
      "options":{"reduceOptions":{"calcs":["lastNotNull"]}},
      "gridPos":{"x":12,"y":0,"w":12,"h":6}
    },
    {
      "title": "Positions",
      "type": "table",
      "targets": [{"expr":"novax_position_qty"}],
      "transformations":[{"id":"labelsToFields"}],
      "gridPos":{"x":0,"y":6,"w":12,"h":6}
    },
    {
      "title": "Prices",
      "type": "table",
      "targets": [{"expr":"novax_last_price"}],
      "transformations":[{"id":"labelsToFields"}],
      "gridPos":{"x":12,"y":6,"w":12,"h":6}
    },
    {
      "title": "Signals per strategi",
      "type": "heatmap",
      "targets": [{"expr":"novax_signal"}],
      "gridPos":{"x":0,"y":12,"w":24,"h":7}
    },
    {
      "title": "Sharpe per strategi",
      "type":"bargauge",
      "targets":[{"expr":"novax_strategy_sharpe"}],
      "options":{"reduceOptions":{"calcs":["lastNotNull"]}},
      "gridPos":{"x":0,"y":19,"w":12,"h":6}
    },
    {
      "title":"Winrate per strategi",
      "type":"bargauge",
      "targets":[{"expr":"novax_strategy_winrate"}],
      "options":{"reduceOptions":{"calcs":["lastNotNull"]}},
      "gridPos":{"x":12,"y":19,"w":12,"h":6}
    }
  ],
  "time":{"from":"now-7d","to":"now"}
}
